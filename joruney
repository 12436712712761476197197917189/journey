Misc:AddToggle("Clear Game", false,function(v)
_G.Clear = v
game:GetService('RunService').Heartbeat:Connect(function()
    if _G.Clear == true then
        game:GetService("ReplicatedStorage").CommandClean:FireServer()
    end
end)
end)
